{
  "name": "@mcp3/transaction-server",
  "version": "1.0.0",
  "description": "Transaction server for handling transactions and serving transaction UI",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "type": "module",
  "bin": {
    "transaction-server": "./build/cli.js"
  },
  "scripts": {
    "build": "tsc && chmod +x build/cli.js",
    "start": "node build/cli.js start",
    "dev": "ts-node src/cli.ts start",
    "clean": "rm -rf build *.tsbuildinfo",
    "prepublishOnly": "pnpm run clean && pnpm run build"
  },
  "keywords": [
    "sui",
    "transaction",
    "server",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui": "^1.24.0",
    "@mcp3/common": "workspace:*",
    "@mcp3/transaction-ui": "workspace:*",
    "body-parser": "^1.20.2",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/commander": "^2.12.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.8.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
