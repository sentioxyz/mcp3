# Sui RPC URL
SUI_RPC_URL=https://fullnode.mainnet.sui.io:443

# Wallet addresses (comma-separated list)
SUI_WALLET_ADDRESSES=0x123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef,0xabcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789

# Wallet names (comma-separated list, in the same order as addresses)
SUI_WALLET_NAMES=Main Wallet,Secondary Wallet

# Default wallet (optional, can be address or name, will use the first one from SUI_WALLET_ADDRESSES if not specified)
SUI_DEFAULT_WALLET=Main Wallet

# Private keys (comma-separated list, in the same order as addresses)
# DO NOT COMMIT WITH REAL KEYS
# SUI_PRIVATE_KEYS=key1,key2

# Mnemonics (comma-separated list, in the same order as addresses)
# DO NOT COMMIT WITH REAL MNEMONICS
# SUI_MNEMONICS=mnemonic1,mnemonic2

# Wallet config file path (optional, defaults to $HOME/.sui-wallet/config.json)
# SUI_WALLET_CONFIG_PATH=./wallets.json
